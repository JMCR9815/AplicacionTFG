<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.Carniceria_TFG"
        android:usesCleartextTraffic="true"
        tools:targetApi="31" >
        <activity
            android:name=".Formularios.Formulario_Productos"
            android:exported="false" />
        <activity
            android:name=".Formularios.FormularioElaboraciones"
            android:exported="false" />
        <activity
            android:name=".Formularios.FormularioCarne"
            android:exported="false" />
        <activity
            android:name=".Activityes.MainActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:exported="true"
            android:theme="@style/Theme.Carniceria_TFG.Fullscreen"
            tools:ignore="DuplicateActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ActionActivity"
            android:exported="false" />
        <activity
            android:name=".Activityes.ElaboracionesActivity"
            android:exported="false" />
        <activity
            android:name=".Activityes.ProveedoresActivity"
            android:exported="false" />
        <activity
            android:name=".Activityes.ProductosActivity"
            android:exported="false" />
        <activity
            android:name=".Activityes.CarneActivity"
            android:exported="false" />
    </application>

</manifest>